    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report",
    "test:cz": "playwright test --project=cz",
    "test:pl": "playwright test --project=pl",
    "test:whitelabel": "playwright test --project=whitelabel",
