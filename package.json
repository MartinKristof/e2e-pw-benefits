    "test:all-projects:headed": "playwright test --project=cz --project=pl --project=whitelabel --headed",
    "format": "prettier --write \"tests/**/*.ts\"",
    "format:check": "prettier --check \"tests/**/*.ts\"",
    "lint": "eslint \"tests/**/*.ts\"",
    "lint:fix": "eslint \"tests/**/*.ts\" --fix"
  },
